{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"h-16": {
		"style": {
			"height": 64
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"flex-row": {
		"style": {
			"flexDirection": "row"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"text-center": {
		"style": {
			"textAlign": "center"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	},
	"text-black": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(0 0 0 / var(--tw-text-opacity))"
		}
	},
	"opacity-50": {
		"style": {
			"opacity": 0.5
		}
	}
}