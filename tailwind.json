{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"h-24": {
		"style": {
			"height": 96
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"flex-initial": {
		"style": {
			"flexGrow": 0,
			"flexShrink": 1,
			"flexBasis": "auto"
		}
	},
	"flex-row": {
		"style": {
			"flexDirection": "row"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-end": {
		"style": {
			"justifyContent": "flex-end"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"bg-black": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(0 0 0 / var(--tw-bg-opacity))"
		}
	},
	"bg-opacity-60": {
		"style": {
			"--tw-bg-opacity": 0.6
		}
	},
	"px-6": {
		"style": {
			"paddingLeft": 24,
			"paddingRight": 24
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"font-semibold": {
		"style": {
			"fontWeight": "600"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	}
}